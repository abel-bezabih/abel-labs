{
  "name": "@abel-labs/api",
  "version": "1.0.0",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
